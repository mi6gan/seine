{
  "name": "seine",
  "description": "Seine is a Javascript library of React components and utilities to view and edit structured content.",
  "homepage": "https://seine.wanttobe.online",
  "version": "0.4.26",
  "keywords": [
    "react",
    "richtext"
  ],
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/mi6gan/seine",
  "scripts": {
    "start": "yarn workspace seine-storybook serve",
    "release": "run-s release:'*'",
    "release:library": "yarn workspaces foreach -t --include='@seine/*' exec npm publish",
    "release:storybook": "yarn workspace seine-storybook build -o $(pwd)/docs",
    "test": "run-s test:'*'",
    "test:library": "yarn eslint --max-warnings=0 --format unix ./packages/*/src",
    "test:storybook": "yarn workspace seine-storybook test",
    "build": "yarn workspaces foreach -t --include='@seine/*' exec yarn rollup --environment NODE_ENV:production -c ../../rollup.config.js",
    "dev": "yarn workspace seine-storybook start"
  },
  "devDependencies": {
    "@babel/core": "7",
    "@devexpress/dx-chart-core": "<=2.7.1",
    "@devexpress/dx-react-chart": "<=2.7.1",
    "@devexpress/dx-react-chart-material-ui": "<=2.7.1",
    "@devexpress/dx-react-core": "<=2.7.1",
    "@material-ui/core": "^4.9",
    "@material-ui/icons": "^4.9",
    "@material-ui/styles": "^4.9",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "@yarnpkg/cli": "^2.2.2",
    "@yarnpkg/core": "^2.2.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.1",
    "eslint": "^4.12.1 || ^5.0.0 || ^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.4.2",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "flow-bin": "^0.109.0",
    "hooks.macro": "^1.0.1",
    "identity-obj-proxy": "^3.0.0",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.13.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "rollup": "^2.18.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-flow-entry": "^0.3.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.2.0",
    "seine-storybook": "portal:./storybook",
    "typescript": "2.x",
    "uuid": "^3.4.0"
  },
  "workspaces": [
    "packages/*",
    "storybook"
  ],
  "dependenciesMeta": {
    "@storybook/core": {
      "unplugged": true
    },
    "flow-bin": {
      "unplugged": true
    }
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  }
}
