{
  "name": "seine",
  "description": "Seine is a Javascript library of React components and utilities to view and edit structured content.",
  "homepage": "https://seine.wanttobe.online",
  "keywords": [
    "react",
    "richtext"
  ],
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/mi6gan/seine",
  "scripts": {
    "start": "serve",
    "test": "run-s test:'*'",
    "test:eslint": "eslint -c .eslintrc.js --format unix packages/",
    "test:jest": "jest --silent",
    "build": "run-s build:'*'",
    "build:cjs": "node scripts/build.js --format=cjs",
    "build:esm": "node scripts/build.js --format=esm",
    "build:umd": "node scripts/build-workspace.js --format=umd --workspace=packages/testbox",
    "nobuild:storybook": "yarn build-storybook --quiet --loglevel silent -o dist",
    "deploy": "run-s deploy:'*'",
    "deploy:build": "docker build -t mi6gan/seine:${npm_package_version} .",
    "deploy:version": "docker push mi6gan/seine:${npm_package_version}",
    "deploy:latest": "docker tag mi6gan/seine:${npm_package_version} mi6gan/seine:latest",
    "deploy:push": "docker push mi6gan/seine:latest",
    "dev": "run-s dev:'*'",
    "dev:start": "start-storybook -p 3000"
  },
  "dependencies": {
    "@devexpress/dx-chart-core": "^2.7.0",
    "@devexpress/dx-react-chart": "^2.7.0",
    "@devexpress/dx-react-chart-material-ui": "^2.7.0",
    "@devexpress/dx-react-core": "^2.7.0",
    "@material-ui/core": "4.9",
    "@material-ui/icons": "4.9",
    "@seine/content": "portal:./packages/content",
    "d3-interpolate": "1.4",
    "draft-js": "^0.11.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "serve": "^11.1.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "7",
    "@jest/transform": "^26.3.0",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-storyshots": "^6.0.21",
    "@storybook/addon-viewport": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.1",
    "colorette": "^1.1.0",
    "cross-env": "^6.0.3",
    "draft-js": "^0.11.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsdoc": "^30.4.2",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "find-cache-dir": "^3.3.0",
    "flow-bin": "^0.109.0",
    "folder-hash": "^3.3.0",
    "glob": "^7.1.4",
    "history": "^4.10.1",
    "hooks.macro": "^1.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^24.9.0",
    "jest-serializer-enzyme": "^1.0.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "pnp-webpack-plugin": "^1.6.4",
    "prettier": "^2.1.1",
    "pretty-ms": "^6.0.0",
    "ramda": "^0.26.1",
    "react-app-polyfill": "^1.0.2",
    "react-test-renderer": "^16.9.0",
    "require-context.macro": "^1.1.1",
    "rollup": "^2.18.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-flow-entry": "^0.3.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-visualizer": "^2.5.4",
    "uuid": "^3.4.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  }
}
