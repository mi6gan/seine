{
  "name": "seine",
  "description": "Seine is a Javascript library of React components and utilities to view and edit structured content.",
  "homepage": "https://seine.wanttobe.online",
  "version": "0.4.2",
  "keywords": [
    "react",
    "richtext"
  ],
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/mi6gan/seine",
  "scripts": {
    "start": "serve",
    "test": "run-s test:'*'",
    "test:eslint": "eslint -c .eslintrc.js --format unix packages/",
    "build": "yarn workspaces foreach -t --include='@seine/*' exec yarn rollup --environment NODE_ENV:production -c ../../rollup.config.js",
    "release": "yarn workspaces foreach -t --include='@seine/*' exec npm publish",
    "deploy": "run-s deploy:'*'",
    "deploy:build": "docker build -t mi6gan/seine:${npm_package_version} .",
    "deploy:version": "docker push mi6gan/seine:${npm_package_version}",
    "deploy:latest": "docker tag mi6gan/seine:${npm_package_version} mi6gan/seine:latest",
    "deploy:push": "docker push mi6gan/seine:latest",
    "dev": "yarn workspace seine-storybook start"
  },
  "dependencies": {
    "@devexpress/dx-chart-core": "<=2.7.1",
    "@devexpress/dx-react-chart": "<=2.7.1",
    "@devexpress/dx-react-chart-material-ui": "<=2.7.1",
    "@devexpress/dx-react-core": "<=2.7.1",
    "@material-ui/core": "^4.9",
    "@material-ui/icons": "^4.9",
    "@seine/editor": "workspace:*",
    "d3-interpolate": "1.4",
    "draft-js": "^0.11.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "serve": "^11.1.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "7",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@yarnpkg/cli": "^2.2.2",
    "@yarnpkg/core": "^2.2.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.1",
    "draft-js": "^0.11.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsdoc": "^30.4.2",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "flow-bin": "^0.109.0",
    "hooks.macro": "^1.0.1",
    "identity-obj-proxy": "^3.0.0",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rollup": "^2.18.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-flow-entry": "^0.3.1",
    "rollup-plugin-pnp-resolve": "^2.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-visualizer": "^2.5.4"
  },
  "workspaces": [
    "packages/*",
    "storybook"
  ],
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "dependenciesMeta": {
    "@storybook/core": {
      "unplugged": true
    }
  }
}
